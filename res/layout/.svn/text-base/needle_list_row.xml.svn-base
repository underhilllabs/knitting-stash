<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content">
<TextView
	android:id="@+id/size_row"
	style="@style/ListRowHeader"

	/>
<TextView
	android:id="@+id/type_row"
	android:layout_width="70dip"
	android:layout_height="wrap_content"
	android:layout_below="@id/size_row"/>
<TextView
	android:id="@+id/material_row"
	android:layout_width="70dip"
	android:layout_height="wrap_content"
	android:layout_below="@id/size_row"
	android:layout_toRightOf="@id/type_row"
	android:gravity="center_horizontal"
	/>
	
<TextView
	android:id="@+id/length_row"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_below="@id/size_row"
	android:layout_toRightOf="@id/material_row"
	android:gravity="right"
	android:layout_alignParentRight="true"
	android:layout_marginRight="10dp"
	/>
	
 
</RelativeLayout>
